<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="186.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.apekshapms.controller.sidebar.LabAssistantSideBarController">
   <children>
      <VBox prefHeight="600.0" prefWidth="186.0" stylesheets="@../../../resources/styles/patientManagement.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="addReportButton" mnemonicParsing="false" onAction="#handleAddReportButtonAction" prefHeight="27.0" prefWidth="145.0" text="Add Report" />
            <Button fx:id="addDetailsButton" mnemonicParsing="false" onAction="#handleAddDetailsButtonAction" prefHeight="27.0" prefWidth="145.0" text="Add Details" />
            <Button fx:id="checkReportButton" mnemonicParsing="false" onAction="#handleCheckReportButtonAction" prefHeight="27.0" prefWidth="145.0" text="Check Report" />
            <HBox prefHeight="28.0" prefWidth="144.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleReceivedMessagesButtonAction" prefHeight="27.0" prefWidth="146.0" text="Received Messages" />
                  <Label fx:id="massageNotificationLable" prefHeight="28.0" prefWidth="32.0" text="Label" textFill="#002bff" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
